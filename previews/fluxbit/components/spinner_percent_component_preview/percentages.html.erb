<div class="grid grid-cols-3 md:grid-cols-6 gap-6">
  <% [0, 20, 40, 60, 80, 100].each do |percent| %>
    <div class="flex flex-col items-center gap-2">
      <%= render Fluxbit::SpinnerPercentComponent.new(percent: percent, color: :success) %>
      <span class="text-sm text-gray-600 dark:text-gray-400"><%= percent %>%</span>
    </div>
  <% end %>
</div>